{
	"root": true,
	"env": {
		"node": true,
    "browser": true,
    "commonjs": true
	},
	"parser": "babel-eslint",
	  "plugins": [
    "react",
    "react-native"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
	"globals": {},
  "rules": {
    "no-use-before-define": ["error", { "functions": false, "classes": false}],
    "no-shadow": 0,
    "no-console":0,
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-new": 2,
    "no-plusplus": 2,
    "no-undef": 2,
    "strict": 0,
    "no-unused-vars": 2,
    "no-loop-func": 2,
    "semi": 0,
    "no-cond-assign": 0,
    "no-debugger": 0,
    "no-eq-null": 0,
    "no-eval": 0,
    "no-unused-expressions": 2,
    "guard-for-in": 0,
    "block-scoped-var": 0,
    "no-iterator": 0,
    "linebreak-style": [
      2,
      "unix"
    ],
    "comma-style": [
      2,
      "last"
    ],
    "quotes": 0,
    "no-multi-str": 0,
    "no-proto": 0,
    "no-script-url": 0,
    "dot-notation": 2,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-invalid-this": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "max-statements": [
      2,
      70
    ],
    "complexity": [
      2,
      25
    ],
    "keyword-spacing": [
      2,
      {
        "overrides": {
          "if": {
            "after": false
          },
          "for": {
            "after": false
          }
        }
      }
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "array-bracket-spacing": [
      2,
      "never",
      {
        "singleValue": true
      }
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "one-var": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "no-spaced-func": 2,
    "yoda": [
      2,
      "never"
    ],
    "no-with": 2,
    "spaced-comment": [
      2,
      "always"
    ],
    "space-infix-ops": 2,
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "eol-last": 2,
    "consistent-this": [
      2,
      "self"
    ]
  },
  "safeContextKeyword": ["self"]
}
